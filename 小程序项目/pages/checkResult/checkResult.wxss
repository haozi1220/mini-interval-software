}
.state_contant  {
  width: 100%;
  height: 86rpx;
  background-color: #f4faff;
  border-radius: 8rpx;
  color: #999;
  box-sizing: border-box;
  padding: 29rpx;
  margin-top: 25rpx;
  margin-bottom: 70rpx;
}
/* 影像件 */

.img_detail  {
  box-sizing: border-box;
  padding-bottom: 50rpx;
}
.img_all_icon  {
  display: flex;
  flex-flow: row;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  margin-top: 25rpx;
}
/* 返回按钮 */
.return_btn  {
  width: 442rpx;
  box-sizing: border-box;
  margin-top: 80rpx;
}
