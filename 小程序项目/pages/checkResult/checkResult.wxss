/* pages/checkResult/checkResult.wxss */
.container  {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  background-color: #eee;
  width: 100%;
  min-height: 100vh;
  padding: 31rpx 22rpx 0;
}
/*  */
.c_r_state  {
  background-color: #fff;
  width: 100%;
  box-sizing: border-box;
  padding:35rpx 29rpx 0;
  font-size: 28rpx;
  color: #333;
}
.c_r_state_f , .c_r_state_t  {
  display: flex;
  flex-flow: row;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  margin-bottom: 60rpx;
}
/* 补充说明 */

